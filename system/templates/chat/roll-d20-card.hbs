<div class="shadowdark chat-card roll-card" data-actor-id="{{data.actor.id}}" {{#if tokenId}}data-token-id="{{tokenId}}"{{/if}}>
  <header class="card-header flexrow">
    <img src="{{data.actor.img}}" title="{{title}}"/>
    <h3 class="roll-name">{{title}}</h3>
  </header>
  {{{rolls.d20.renderedHTML}}}
  <div class="d20-roll card-roll blindable" data-blind="{{rolls.blindroll}}">
    {{#if rolls.d20}}
      <div>{{{rolls.d20.renderedHTML}}}</div>
    {{/if}}
  </div>
</div>