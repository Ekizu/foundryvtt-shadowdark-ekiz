<div class="shadowdark chat-card roll-card" data-actor-id="{{data.actor.id}}" {{#if tokenId}}data-token-id="{{tokenId}}"{{/if}}>
  <header class="card-header flexrow">
    <img src="{{data.actor.img}}" title="{{title}}"/>
    <h3 class="roll-name">{{title}}</h3>
  </header>
  <div class="roll card-roll blindable" data-blind="{{data.rolls.main.blind}}">
    {{#if data.rolls.main}}
      <div>{{{data.rolls.main.renderedHTML}}}</div>
    {{/if}}
  </div>
</div>
