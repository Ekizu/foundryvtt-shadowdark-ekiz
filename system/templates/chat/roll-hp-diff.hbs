<div class="shadowdark chat-card roll-card" data-actor-id="{{actor.id}}" {{#if tokenId}}data-token-id="{{tokenId}}"{{/if}}>
  <header class="card-header flexrow">
    <img src="{{actor.img}}" title="{{title}}"/>
    <h3 class="roll-name">{{title}}</h3>
  </header>
  <div>
    {{previous}}
  </div>
  {{#each results as |result|}}
  <div class="roll card-roll">
    {{result.chatCardData}}
  </div>
  {{/each}}
  <div>
    {{total}}
  </div>
</div>